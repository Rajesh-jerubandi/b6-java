pipeline{
    agent any
    tools{
        maven 'm1'
    }
    stages {
stage('test') {
    steps {
      sh 'mvn test'
    }
  }
  stage('clean') {
    steps {
      sh 'mvn clean'
    }
  }

  stage('install') {
    steps {
      sh 'mvn install'
    }
  }


  stage('run') {
    steps {
        sh 'chmod 777 ./scripts/deliver.sh'
      sh './scripts/deliver.sh'
    }
  }

}

}
